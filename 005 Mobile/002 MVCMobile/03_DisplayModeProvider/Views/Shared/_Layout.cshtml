<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <style>
        table
        {
            border-collapse: collapse;
            margin-left: 50px;
        }

        th
        {
            text-align: left;
            border-bottom: 1px solid black;
            padding-right: 20px;
            font-weight: normal;
        }

        td
        {
            padding-left: 10px;
            border-bottom: 1px solid black;
        }

        h2
        {
            color: #ff0085;
        }
    </style>
</head>
<body>
    @RenderBody()

    <br />
    <br />
    <br />
    User-Agents
    <table>
        <tr>
            <td>Opera Mobile</td>
            <td>Opera/12.02 (Android 4.1; Linux; Opera Mobi/ADR-1111101157; U; en-US) Presto/2.9.201 Version/12.02</td>
        </tr>
        <tr>
            <td>Silk</td>
            <td>Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10_6_3; en-us; Silk/1.1.0-80) AppleWebKit/533.16 (KHTML, like Gecko) Version/5.0 Safari/533.16 Silk-Accelerated=true</td>
        </tr>
        <tr>
            <td>IPad</td>
            <td>Mozilla/5.0 (iPad; U; CPU OS 3_2 like Mac OS X; en-us) AppleWebKit/531.21.10 (KHTML, like Gecko) Version/4.0.4 Mobile/7B334b Safari/531.21.10</td>
        </tr>
    </table>
</body>
</html>
